{
   "name": "@diagrams/main",
   "version": "0.1.1",
   "private": true,
   "homepage": "https://joewood.github.io/dag-3d",
   "workspaces": [
      "./example",
      "./graph"
   ],
   "devDependencies": {
      "typescript": "latest"
   },
   "dependencies": {
      "concurrently": "^7.0.0"
   },
   "scripts": {
      "start": "concurrently --kill-others -n graph,example npm:start-graph npm:start-example",
      "start-graph": "FORCE_COLORS npm start -w graph | cat",
      "start-example": "FORCE_COLORS npm start -w example | cat"
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "prettier": {
      "tabWidth": 3,
      "printWidth": 120
   }
}
